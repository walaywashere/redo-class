# Sanity Configuration
# Copy this file to .env.local and fill in your actual values

# Required: Your Sanity Project ID (get this from sanity.io/manage)
NEXT_PUBLIC_SANITY_PROJECT_ID=your_project_id_here

# Required: Sanity Dataset (usually 'production')
NEXT_PUBLIC_SANITY_DATASET=production

# Optional: Read token for private content or enhanced security
SANITY_API_READ_TOKEN=your_read_token_here

# Optional: Preview mode secret for draft content
SANITY_PREVIEW_SECRET=your_preview_secret_here 